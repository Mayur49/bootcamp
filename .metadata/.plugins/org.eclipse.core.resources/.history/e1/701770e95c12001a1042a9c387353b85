import java.util.Scanner;

public class gambler {

	public static void main(String[] args) {
		
		int n,goal,cash,iteration,i;
		float count=0,precentage;
		 
		Scanner sc = new Scanner(System.in);
		
		cash= sc.nextInt();
		
      System.out.println("enter the iteration");
      iteration= sc.nextInt();
		
      System.out.println("enter the goal");
      goal= sc.nextInt();

	for(i=0;i<n;i++)
	{
		n=(int) Math.random();
		if(n==1)
		{
			cash=cash+50;
			count++;
		}
		else
		{
			cash=cash-50;
		}
	}
	
	 System.out.println("number of wins"+count);
	 
	 if(cash==goal || cash>=goal)
	 {
		 System.out.println("win");
	 }
	 else {
		 System.out.println("loss");
	      
	 }
   float percentage=iteration*(count/100);
	 
   System.out.println("percentage of wins"+percentage);
	
   System.out.println("percentage of loss"+(100-percentage));
	
	
	}

}
