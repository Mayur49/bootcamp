
public class vendingmachine {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       int n=12345;
       int a[];
       int amt1=0,i=0;
       int amt[]= {234,345,1098,2345,321,123,1253};
       if(n>0)
       {
    	   amt1=n / amt[i];
    	   amt1=n % amt[i];
    	   i++;
    	   System.out.println(amt1);
       }
	}

}
