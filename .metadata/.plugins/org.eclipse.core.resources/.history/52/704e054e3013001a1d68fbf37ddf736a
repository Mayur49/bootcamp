import java.util.Scanner;

public class pascal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j,k,c=1;
		int rows;
		 
		 Scanner sc=new Scanner(System.in);
		 System.out.println("enter the rows");
		rows=sc.nextInt();
		
		for(i=0;i<rows;i++)
		{
			for(j=1;j<rows-1;j++)
				System.out.println("");
			for(k=0;k<=i;k++)
			{
				if(i==0 || j==0)
					c=1;
		    
				else
			
			c=c*(i-j+1)/j;	
			System.out.println(""+c);
			}
			System.out.println("\n");
			}
	
	}

}
